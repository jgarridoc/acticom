<p id="notice"><%= notice %></p>

<h1>Unidades</h1>

<div class="table-responsive">
<table class="table table-striped table-sm">
  <thead>
    <tr>
      <th scope="col">Tipo</th>
      <th scope="col">Profile</th>
      <th scope="col">Nombre calle</th>
      <th scope="col">Numero calle</th>
      <th scope="col">Comuna</th>
      <th scope="col">Ciudad</th>
      <th scope="col">Region</th>
      <th scope="col">Zip code</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @units.each do |unit| %>
      <tr>
        <td><%= unit.tipo %></td>
        <td><%= unit.profile_id %></td>
        <td><%= unit.nombre_calle %></td>
        <td><%= unit.numero_calle %></td>
        <td><%= unit.comuna %></td>
        <td><%= unit.ciudad %></td>
        <td><%= unit.region %></td>
        <td><%= unit.zip_code %></td>
        <td><%= link_to 'Ver', unit %></td>
        <td><%= link_to 'Editar', edit_unit_path(unit) %></td>
        <td><%= link_to 'Eliminar', unit, method: :delete, data: { confirm: '¿Está segur@?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
<br>

<%= link_to 'Nueva unidad', new_unit_path %>
